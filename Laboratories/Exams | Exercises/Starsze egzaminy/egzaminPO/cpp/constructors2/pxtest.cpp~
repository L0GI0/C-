#include <iostream>
using namespace std;

class A
{
public:
  A(){cout << "Konstruktor A" << endl;}
  ~A(){cout << "Destruktor A" << endl;}
};

class B
{
public:
  B(){cout << "Konstruktor B" << endl;}
  ~B(){cout << "Destruktor B" << endl;}
};


class C
{
public:
  C(){cout << "Konstruktor C" << endl;}
  ~C(){cout << "Destruktor C" << endl;}
};


class D: public C 
{
public:
  D(){cout << "Konstruktor D" << endl;}
  ~D(){cout << "Destruktor D" << endl;}
};


int main()
{
D obiekt1;;

}
